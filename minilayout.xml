<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="cros"
           fetch="http://src.chromium.org/git"
           review="codereview.chromium.org" />
  <default revision="refs/heads/master"
           remote="cros" />

  <project path="src/overlays" name="board-overlays" />
  <project path="src/scripts" name="crosutils" />

  <project path="deprecated" name="chromiumos" >
    <copyfile src="WATCHLISTS" dest="WATCHLISTS" />
    <copyfile src="codereview.settings" dest="codereview.settings" />
    <copyfile src="PRESUBMIT.py" dest="PRESUBMIT.py" />
  </project>

  <project path="src/third_party/portage" name="portage" />
  <project path="src/third_party/chromiumos-overlay" name="chromiumos-overlay" />
  <project path="src/third_party/crosstool-overlay" name="crosstool-overlay" />
<!--These need to be removed -->
  <project path="src/platform/installer" name="installer" />
  <project path="src/third_party/hardened-dev" name="hardened-dev" />
  <project path="src/third_party/wpa_supplicant" name="hostap" />
</manifest>
